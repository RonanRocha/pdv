<Window x:Class="EmissorNF.Cliente.Telas.Caixa.WFBuscaProdutos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmissorNF.Cliente.Telas.Caixa"
        mc:Ignorable="d"
        Title="Busca de Produtos" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="9*"></RowDefinition>
        </Grid.RowDefinitions>
        <WrapPanel Grid.Row="0" Margin="5" VerticalAlignment="Center">
            <TextBox  MinWidth="50" Margin="3"></TextBox>
            <TextBox  MinWidth="350" Margin="3"></TextBox>
            <Button Margin="3">Adicionar</Button>
        </WrapPanel>
        <DataGrid Grid.Row="2" Margin="5"></DataGrid>
    </Grid>
</Window>
